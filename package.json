{
  "name": "template-electron-angular",
  "version": "0.0.0",
  "description": "Electron+Angular template project for PC client.",
  "scripts": {
    "start:ng": "ng serve",
    "start:electron": "tsc -p src/electron/tsconfig.json && (export NODE_ENV=debug && electron .)",
    "build:ng-prod": "ng build --configuration production",
    "build:electron": "tsc -p src/electron/tsconfig.json",
    "release": "./scripts/release.sh",
    "generate-res": "./scripts/generate-res.sh",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "main": "dist/electron/main.js",
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/router": "~13.1.0",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.1",
    "@angular/cli": "~13.1.1",
    "@angular/compiler-cli": "~13.1.0",
    "@angular/language-service": "~13.1.0-next.3",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~12.20.37",
    "codelyzer": "^6.0.0",
    "electron": "^9.4.4",
    "electron-builder": "^22.14.5",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ngx-electron": "^2.2.0",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.5.3"
  }
}
